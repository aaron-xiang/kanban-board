{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown"
  ],
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": ["source.fixAll.eslint", "source.formatDocument"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  }
}
